<?xml version="1.0" encoding="UTF-8"?>
<SBHC:Configuration
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:SBHC="Configuration.SBHModel.storagebenchmarkharness.sdq.kit.edu"
    xmlns:SBHCO="Operations.Configuration.SBHModel.storagebenchmarkharness.sdq.kit.edu">
  <experimentSetup
      identifier="Mailserver_and_Fileserveron2Host"
      repeatCount="3">
    <experimentSeries
        independentVariableSpaceOfBenchmark="FilebenchFileserver"
        independentVariableSpaceOfSut="NOOP+ext4"
        systemUnderTest="SUT_EX1"
        identifier="Fileserver_Run"/>
    <experimentSeries
        independentVariableSpaceOfBenchmark="FilebenchMailserver"
        independentVariableSpaceOfSut="NOOP+ext4"
        systemUnderTest="SUT_EX2"
        identifier="Mailserver_Run"/>
  </experimentSetup>
  <systemUnderTestRepository>
    <systemsUnderTest
        identifier="SUT_EX1"
        ip="IP.IP.IP.IP"
        port="0"
        user="username"
        keyFile="/home/reeb/.ssh/id_rsa"/>
    <systemsUnderTest
        identifier="SUT_EX2"
        ip="IP.IP.IP.IP"
        port="0"
        user="username"
        keyFile="/Path/RSA_KEY_FILE"/>
  </systemUnderTestRepository>
  <independentVariableSpaceRepository>
    <independentVariableSpaces
        xsi:type="SBHC:IndependentVariableSpaceOfSut"
        identifier="NOOP+ext4">
      <fileSystem>ext4</fileSystem>
      <scheduler>NOOP</scheduler>
    </independentVariableSpaces>
    <independentVariableSpaces
        xsi:type="SBHC:IndependentVariableSpaceOfFilebench"
        identifier="FilebenchFileserver"
        name="Fileserver">
      <threads
          threadName="fileserver_thread">
        <memsize>10m</memsize>
        <operations
            xsi:type="SBHCO:CreateFile"
            operationName="createfile1"
            flowOpName="createfile"
            warmupOperation="false"
            filesetname="fileserverFileset"
            fd="1">
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:WriteHoleFile"
            operationName="wrtfile1"
            flowOpName="writewholefile"
            warmupOperation="false"
            filesetname=""
            workingset=""
            fd="1"
            srcfd="1">
          <iosize>1m</iosize>
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:CloseFile"
            operationName="closefile1"
            flowOpName="closefile"
            warmupOperation="false"
            fd="1"/>
        <operations
            xsi:type="SBHCO:OpenFile"
            operationName="openfile1"
            flowOpName="openfile"
            filesetname="fileserverFileset"
            fd="1">
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:AppendFileRand"
            operationName="appendfilerand1"
            flowOpName="appendfilerand"
            filesetname=""
            fd="1">
          <iosize>16k</iosize>
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:CloseFile"
            operationName="closefile2"
            flowOpName="closefile"
            fd="1"/>
        <operations
            xsi:type="SBHCO:OpenFile"
            operationName="openfile2"
            flowOpName="openfile"
            filesetname="fileserverFileset"
            fd="1">
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:ReadWholeFile"
            operationName="readfile1"
            flowOpName="readwholefile"
            fileset=""
            fd="1">
          <iosize>1m</iosize>
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:CloseFile"
            operationName="closefile3"
            flowOpName="closefile"
            fd="1"/>
        <operations
            xsi:type="SBHCO:DeleteFile"
            operationName="deletefile1"
            flowOpName="deletefile"
            warmupOperation="false"
            filesetname="fileserverFileset"/>
        <operations
            xsi:type="SBHCO:StatFile"
            operationName="statfile1"
            flowOpName="statfile"
            filesetname="fileserverFileset"/>
        <instances>40</instances>
        <instances>40</instances>
        <instances>40</instances>
        <instances>40</instances>
      </threads>
      <filesets
          filesetName="fileserverFileset">
        <directory>/fileserverFileset</directory>
        <meanDirWidth>20</meanDirWidth>
        <files>10000</files>
        <meanFileSize>128k</meanFileSize>
        <prealloc>80</prealloc>
      </filesets>
      <runTime>300</runTime>
      <warmupTime>60</warmupTime>
    </independentVariableSpaces>
    <independentVariableSpaces
        xsi:type="SBHC:IndependentVariableSpaceOfFilebench"
        identifier="FilebenchMailserver"
        name="Mailserver">
      <threads
          threadName="Mailserver_Thread">
        <memsize>10m</memsize>
        <operations
            xsi:type="SBHCO:DeleteFile"
            operationName="deletefile1"
            flowOpName="deletefile"
            warmupOperation="false"
            filesetname="mailserverFileset"/>
        <operations
            xsi:type="SBHCO:CreateFile"
            operationName="createfile2"
            flowOpName="createfile"
            warmupOperation="false"
            filesetname="mailserverFileset"
            fd="1">
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:AppendFileRand"
            operationName="appendfilerand2"
            flowOpName="appendfilerand"
            warmupOperation="false"
            filesetname=""
            fd="1">
          <iosize>32k</iosize>
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:CloseFile"
            operationName="closefile2"
            flowOpName="closefile"
            warmupOperation="false"
            fd="1"/>
        <operations
            xsi:type="SBHCO:OpenFile"
            operationName="openfile3"
            flowOpName="openfile"
            filesetname="mailserverFileset"
            fd="1">
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:ReadWholeFile"
            operationName="readfile3"
            flowOpName="readwholefile"
            fileset=""
            fd="1">
          <iosize>1m</iosize>
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:CloseFile"
            operationName="closefile3"
            flowOpName="closefile"
            fd="1"/>
        <operations
            xsi:type="SBHCO:OpenFile"
            operationName="openfile4"
            flowOpName="openfile"
            warmupOperation="false"
            filesetname="mailserverFileset"
            fd="1">
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:AppendFileRand"
            operationName="appendfilerand4"
            flowOpName="appendfilerand"
            warmupOperation="false"
            filesetname=""
            fd="1">
          <iosize>32k</iosize>
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:CloseFile"
            operationName="closefile4"
            flowOpName="closefile"
            warmupOperation="false"
            fd="1"/>
        <operations
            xsi:type="SBHCO:OpenFile"
            operationName="openfile5"
            flowOpName="openfile"
            filesetname="mailserverFileset"
            fd="1">
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:ReadWholeFile"
            operationName="readfile5"
            flowOpName="readwholefile"
            fileset=""
            fd="1">
          <iosize>1m</iosize>
          <directio>1</directio>
        </operations>
        <operations
            xsi:type="SBHCO:CloseFile"
            operationName="closefile5"
            flowOpName="closefile"
            fd="1"/>
        <instances>10</instances>
        <instances>20</instances>
        <instances>30</instances>
        <instances>40</instances>
      </threads>
      <filesets
          filesetName="mailserverFileset">
        <directory>/mailserverFileset</directory>
        <meanDirWidth>1000000</meanDirWidth>
        <files>50000</files>
        <meanFileSize>16k</meanFileSize>
        <prealloc>80</prealloc>
      </filesets>
      <runTime>300</runTime>
      <warmupTime>60</warmupTime>
    </independentVariableSpaces>
  </independentVariableSpaceRepository>
</SBHC:Configuration>
